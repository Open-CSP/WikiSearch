{
	"name": "SMWS",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:SMWS",
	"descriptionmsg": "swms-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"SMWS": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.app": {
			"dependencies": [
				"ext.vue"
			],
			"scripts": [
						"app.js"
					],
					"styles": [
						"app.css"
					]
			},
			"ext.vue": {
				"scripts": [
							"vue.js"
						]
			}
	},
		"ResourceFileModulePaths": {
		"localBasePath": "src",
		"remoteExtPath": "SMWS/src"
	},
	"AutoloadClasses": {
		"SMWSHooks": "src/SMWSHooks.php",
		"ApiQuerySMWS": "src/ApiQuerySMWS.php",
		"WSSearch": "src/WSSearch.php"
	},
	"ExtensionMessagesFiles": {
		"SMWSMagic": "SMWS.i18n.php"
	},
	"APIMetaModules": {
	  "smws": "ApiQuerySMWS"
  },
	"Hooks": {
		"ParserFirstCallInit": "SMWSHooks::onParserFirstCallInit",
		"BeforePageDisplay": [
			"SMWSHooks::onBeforePageDisplay"
		]
	},
	"manifest_version": 1
}
