{
	"name": "WSSearch",
	"author": "Me",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:WSSearch",
	"descriptionmsg": "wssearch-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WSSearch": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.app": {
			"dependencies": [
				"ext.vue"
			],
			"scripts": [
						"app.js"
					],
					"styles": [
						"app.css"
					]
			},
			"ext.vue": {
				"scripts": [
							"vue.js"
						]
			}
	},
		"ResourceFileModulePaths": {
		"localBasePath": "src",
		"remoteExtPath": "WSSearch/src"
	},
	"AutoloadClasses": {
		"WSSearchHooks": "src/WSSearchHooks.php",
		"ApiQueryWSSearch": "src/ApiQueryWSSearch.php",
		"WSSearch": "src/WSSearch.php"
	},
	"ExtensionMessagesFiles": {
		"WSSearchMagic": "WSSearch.i18n.php"
	},
	"APIMetaModules": {
	  "WSSearch": "ApiQueryWSSearch"
  },
	"Hooks": {
		"ParserFirstCallInit": "WSSearchHooks::onParserFirstCallInit",
		"BeforePageDisplay": [
			"WSSearchHooks::onBeforePageDisplay"
		]
	},
	"manifest_version": 1
}
